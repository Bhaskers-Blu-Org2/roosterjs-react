{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "../../dist/roosterjs-react-command-bar",
    "noUnusedLocals": true,
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "baseUrl": "../../",
    "paths": {
      "*": [
        "*",
        "dist/*"
      ]
    },
    "rootDir": ".",
    "jsx": "react",
    "lib": [
      "es6",
      "dom"
    ]
  },
  "include": [
    "lib/**/*.ts",
    "lib/**/*.tsx"
  ]
}