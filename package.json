    "@types/react": "~15.6.0",
    "autoprefixer": "^7.1.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.7",
    "node-sass": "4.6.0",
    "postcss-inline-rtl": "^0.9.8",
